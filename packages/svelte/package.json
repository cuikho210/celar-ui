{
	"name": "@celar-ui/svelte",
	"version": "1.8.0",
	"license": "MIT",
	"author": {
		"name": "cuikho210",
		"email": "tonykms210@gmail.com",
		"url": "https://github.com/cuikho210"
	},
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json && eslint .",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"npm-publish": "bun run build && export $(grep -v '^#' ../../.env | xargs) && npm publish --access=public",
		"gen-colors": "bun run ./scripts/gen-colors.ts"
	},
	"files": [
		"dist",
		"src/styles",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./src/styles/*.scss": {
			"import": "./src/styles/*.scss",
			"require": "./src/styles/*.scss"
		}
	},
	"peerDependencies": {
		"bits-ui": "^2.8.0",
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.1",
		"@eslint/js": "^9.39.1",
		"@iconify-json/hugeicons": "^1.2.17",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/kit": "^2.48.4",
		"@sveltejs/package": "^2.5.4",
		"@sveltejs/vite-plugin-svelte": "^5.1.1",
		"@types/bun": "^1.3.1",
		"autoprefixer": "^10.4.21",
		"bits-ui": "^2.14.2",
		"csstype": "^3.1.3",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.0",
		"globals": "^16.5.0",
		"material-dynamic-colors": "^1.1.2",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"publint": "^0.3.15",
		"sass": "^1.93.3",
		"svelte": "^5.43.4",
		"svelte-check": "^4.3.3",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.3",
		"unplugin-icons": "^22.5.0",
		"vite": "^6.4.1"
	},
	"keywords": [
		"svelte"
	],
	"packageManager": "bun@1.1.0"
}
